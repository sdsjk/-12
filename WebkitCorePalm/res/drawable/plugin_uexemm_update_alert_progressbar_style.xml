<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="@color/plugin_uexemm_load_app_progress_bar_bg_color"
                android:startColor="@color/plugin_uexemm_load_app_progress_bar_bg_color"/>
            <corners android:radius="5.0dip"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip"/>
                <gradient
                    android:angle="270"
                    android:endColor="#717171"
                    android:startColor="#9d9d9d"/>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip"/>
                <gradient
                    android:angle="270"
                    android:endColor="#1693ff"
                    android:startColor="#0042b4"/>
            </shape>
        </clip>
    </item>
</layer-list>
