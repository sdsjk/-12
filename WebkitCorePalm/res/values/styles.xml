<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <style name="activity_anim_theme" parent="@android:Theme.NoTitleBar">
        <item name="@android:windowAnimationStyle">@style/activity_translate_anim</item>
    </style>
    
    <style name="activity_translate_anim">
        <item name="android:activityOpenEnterAnimation">@anim/window_open_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/window_open_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/window_close_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/window_close_exit</item>
    </style>
    <style name="appraise_ratingbar_style" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/plugin_appstoremgr_appraise_ratingbar</item>
        <item name="android:minHeight">18dip</item>
        <item name="android:maxHeight">18dip</item>
    </style>
    
    <style name="appraise_submit_ratingbar_style" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/plugin_appstoremgr_submit_ratingbar</item>
        <item name="android:minHeight">50dip</item>
        <item name="android:maxHeight">50dip</item>
    </style>

    <style name="plugin_appstoremgr_loading_dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
	


  <style name="plugin_uexweixin_Transparent">
    <item name="android:windowBackground">@color/plugin_uexweixin_transparent_background</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>
  </style>

   <style name="Style_plugin_uexActionSheet_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="plugin_uex_image_appTheme" parent="android:Theme.Light">
        <item name="android:windowNoTitle">true</item>
    </style>


    <style name="Style.plugin_file_item_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/plugin_file_cb_bg_selector</item>
    </style>  


    <style name="plugin_emm_query_user_cert_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>



    <style name="Anim_plugin_control_input_dialog" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/plugin_control_inputdialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/plugin_control_inputdialog_exit_anim</item>
    </style>
    


    <!-- plugin contacts use -->
    <style name="Style.plugin_contacts_item_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/plugin_contacts_cb_bg_selector</item>
    </style>


    <style name="plugin_uexAliPay_AlertDialog" parent="android:Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/plugin_uexalipay_popup_bg</item>
    </style>
    

     <!-- uexCalendarView start-->
    
    <style name="Widget">
        <item name="android:textAppearance">?android:attr/textAppearance</item>
    </style>

    <style name="Widget.CalendarView">
        <item name="cv_showWeekNumber">true</item>
        <item name="cv_firstDayOfWeek">1</item>
        <item name="cv_minDate">01/01/1900</item>
        <item name="cv_maxDate">12/31/2100</item>
        <item name="cv_shownWeekCount">6</item>
        <item name="cv_selectedDateVerticalBar">@drawable/plugin_uexcalendarview_dayline_holo</item>
        <item name="cv_weekDayTextAppearance">@style/TextAppearance.Small.CalendarViewWeekDayView</item>
        <item name="cv_dateTextAppearance">?android:attr/textAppearanceSmall</item>
    </style>

    <style name="Widget.Holo.CalendarView" parent="Widget.CalendarView">
        <item name="cv_selectedWeekBackgroundColor">#330099FF</item>
        <item name="cv_focusedMonthDateColor">#FFFFFFFF</item>
        <item name="cv_unfocusedMonthDateColor">#66FFFFFF</item>
        <item name="cv_weekNumberColor">#33FFFFFF</item>
        <item name="cv_weekSeparatorLineColor">#19FFFFFF</item>
        <item name="cv_selectedDateVerticalBar">@drawable/plugin_uexcalendarview_dayline_holo</item>
        <item name="cv_weekDayTextAppearance">@style/TextAppearance.Holo.CalendarViewWeekDayView</item>
        <item name="cv_dividerHorizontal">@drawable/plugin_uexcalendarview_list_divider_holo_dark</item>
    </style>

    <style name="Widget.Holo.Light.CalendarView" parent="Widget.CalendarView">
        <item name="cv_selectedWeekBackgroundColor">#330066ff</item>
        <item name="cv_focusedMonthDateColor">#FF000000</item>
        <item name="cv_unfocusedMonthDateColor">#7F08002B</item>
        <item name="cv_weekNumberColor">#7F080021</item>
        <item name="cv_weekSeparatorLineColor">#7F08002A</item>
        <item name="cv_weekDayTextAppearance">@style/TextAppearance.Holo.Light.CalendarViewWeekDayView</item>
        <item name="cv_dividerHorizontal">@drawable/plugin_uexcalendarview_list_divider_holo_light</item>
    </style>

    <style name="TextAppearance.Small.CalendarViewWeekDayView" parent="@android:style/TextAppearance.Small">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.Holo.CalendarViewWeekDayView" parent="TextAppearance.Small.CalendarViewWeekDayView">
        <item name="android:textColor">#505050</item>
    </style>

    <style name="TextAppearance.Holo.Light.CalendarViewWeekDayView" parent="TextAppearance.Small.CalendarViewWeekDayView">
    </style>
    <!-- uexCalendarView end -->
    

	<style name="browser_main_theme" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:scrollbarThumbHorizontal">@drawable/engine_scroller_thumb_hor</item>
        <item name="android:scrollbarThumbVertical">@drawable/engine_scroller_thumb_ver</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="browser_loading_theme" parent="@android:style/Theme.NoTitleBar">
        <item name="android:scrollbarThumbHorizontal">@drawable/engine_scroller_thumb_hor</item>
        <item name="android:scrollbarThumbVertical">@drawable/engine_scroller_thumb_ver</item>
        <item name="android:windowBackground">@drawable/startup_bg_16_9</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">false</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!-- WidgetOne use -->
    <style name="Style_platform_no_title_and_background" parent="android:Theme">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Style_platform_full_screen_no_title_background" parent="android:Theme">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="Style_platform_window_button_text">
        <item name="android:textStyle">normal</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">2</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <style name="Style_platform_myspace_title_text">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#000000</item>
        <item name="android:shadowColor">#FFFFFFFF</item>
        <item name="android:shadowDx">2</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <style name="Style_platform_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Anim_platform_window_actionsheet_dialog" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/platform_window_actionsheet_pull_up_anim</item>
        <item name="android:windowExitAnimation">@anim/platform_window_actionsheet_push_down_anim</item>
    </style>

    <style name="Style_platform_activity_transparent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <style name="Anim_platform_myspace_fade" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/platform_myspace_fade_in_anim</item>
        <item name="android:windowExitAnimation">@anim/platform_myspace_fade_out_anim</item>
    </style>

</resources>
